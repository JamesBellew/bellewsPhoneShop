@model IEnumerable<bellewsPhoneShop.Models.Charger>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.chargerName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.chargerPort)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.chargerPrice)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.chargerName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.chargerPort)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.chargerPrice)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.chargerId">Edit</a> 
                <a asp-action="Details" asp-route-id="@item.chargerId">Details</a> 
                <a asp-action="Delete" asp-route-id="@item.chargerId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
